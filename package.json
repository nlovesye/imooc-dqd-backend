{
  "name": "imooc-dqd-backend",
  "version": "1.0.0",
  "description": "慕课网大前端-koa",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node src/index.js",
    "webpack:debug": "node --inspect-brk ./node_modules/.bin/webpack --inline --progress",
    "dev": "cross-env NODE_ENV=dev nodemon --exec babel-node --inspect=7100 ./src/index.js",
    "build": "cross-env NODE_ENV=prod webpack --config config/webpack.config.prod.js",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlovesye/imooc-dqd-koa.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nlovesye/imooc-dqd-koa/issues"
  },
  "homepage": "https://github.com/nlovesye/imooc-dqd-koa#readme",
  "dependencies": {
    "@koa/cors": "3.3.0",
    "koa": "2.13.4",
    "koa-body": "5.0.0",
    "koa-combine-routers": "4.0.2",
    "koa-compress": "5.1.0",
    "koa-helmet": "6.1.0",
    "koa-json": "2.0.2",
    "koa-router": "11.0.1",
    "koa-static": "5.0.0",
    "nodemailer": "6.7.7",
    "svg-captcha": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/node": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@types/nodemailer": "6.4.4",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "cross-env": "7.0.3",
    "husky": "8.0.1",
    "nodemon": "2.0.19",
    "rimraf": "3.0.2",
    "terser-webpack-plugin": "5.3.3",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  }
}
